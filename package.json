    "@types/mongoose": "^5.3.11",
    "copyfiles": "^2.1.0",
    "mongoose": "^5.4.8",
    "rimraf": "^2.6.3",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  },
  "dependencies": {}
}
